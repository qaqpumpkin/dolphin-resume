{
  "name": "dolphin-resume",
  "version": "0.0.1",
  "author": "pumpkin",
  "description": "简历平台",
  "main": "./dist/electron.js",
  "scripts": {
    "start:main": "webpack --config ./webpack/webpack.main.dev.js && electron ./dist/electron.js",
    "start:render": "npm run build:dll && webpack-dev-server --config ./webpack/webpack.render.dev.js",
    "build:main": "webpack --config ./webpack/webpack.main.prod.js",
    "build:render": "webpack --config ./webpack/webpack.render.prod.js",
    "build:dll": "webpack --config ./webpack/webpack.dll.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "rm -rf dist package && npm run build:main && npm run build:dll && npm run build:render && npm run dist",
    "install:electron": "ELECTRON_MIRROR=https://cdn.npm.taobao.org/dist/electron/ npm install electron"
  },
  "build": {
    "appId": "dolphinResume.ElectronReact",
    "productName": "DolphinResume",
    "copyright": "Copyright © 2022 ${author}",
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/"
    ],
    "directories": {
      "output": "package"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "assets/icon/dolphin.ico"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@types/redux-logger": "^3.0.9",
    "babel-plugin-import": "^1.13.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.5",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "antd": "^4.24.4",
    "autoprefixer": "^9.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-css-modules": "^5.2.6",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^3.0.0",
    "electron": "^11.1.1",
    "electron-builder": "^23.6.0",
    "eslint": "^7.26.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.6.2",
    "moment": "^2.29.4",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-less": "^4.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.3.0",
    "rc-redux-model": "^1.1.10",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.2.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
